    "@typescript-eslint/parser": "^5.2.0",
    "builtin-modules": "^3.2.0",
    "esbuild": "0.13.12",
    "obsidian": "latest",
    "tslib": "2.3.1",
    "typescript": "4.4.4"
  }
}